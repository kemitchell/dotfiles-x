#!/usr/bin/env node
var exec = require('child_process').execSync
var readout = exec('acpi -b').toString()
var re = /(\d+)%/
process.stdout.write(
  readout
    .split(require('os').EOL)
    .reduce(function(percentages, line) {
      var match = re.exec(line)
      if (match) {
        percentages.push(match[1] + '%') }
      return percentages
    }, [])
    .join(', '))
